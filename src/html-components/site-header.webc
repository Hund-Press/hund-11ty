<link rel="stylesheet" webc:scoped href="../styles/site-header.css" />

<header webc:root="override">
  <div id="site-title">
    <a @raw="site.name" :href="/"></a>
  </div>
  <nav id="site-nav">
    <span webc:for="nav of site.nav">
      <a @raw="nav.label" :href="nav.url"></a>
    </span>
  </nav>
</header>
