<link rel="stylesheet" href="../styles/post-full.css" webc:scoped/>

<article webc:root="override" class="post-full">
  <h1 webc:if="title" @raw="title"></h1>
  <p webc:if="abstract" @raw="abstract" class="post-abstract"></p>
  <template @raw="post" webc:nokeep></template>
  <datetime @raw="formatPostDate(gremlin)"></datetime>
</article>

