<link rel="stylesheet" webc:scoped href="../styles/site-header.css" />

<header webc:root="override">
  <div id="site-name"><a @raw="site.name" href="/"></a></div>
  <nav id="site-nav">
    <template webc:nokeep webc:for="item of site.nav">
      <a :href="item.url"><span @text="item.label"></span></a>
    </template>
  </nav>
</header>
